<div class=“contents_forall”>
<h1> <%= @user.first_name %> <small>time for a </small>
    <a href="/entry/new"><button type="button" class="btn btn-primary">New Post</button></a></h1>
<% if @entries.empty? %>
<p>
If you are not seeing any posts. Contact <a href=“mailto:lex83nyc@gmail.com”>Lex</a> ASAP!
</p>		
<% else %>
  
<h3>Latest Posts</h3>

        <% @entries.each do |entry| %>
        <div class="col-xs-8 col-sm-6 col-md-6 form_remix">
        <span class="nickname_display"><%= entry.user.nickname %></span>
        <br>
        <span class="title_display"><%= entry.title %></span>
        <br>
        <span class="message_display"><%= entry.message %></span>

        <% if entry.user_id == session[:user_id] %>
        <div>
        <small><a href="/entry/edit/<%= entry.id %>">Edit</a>
        | <a href="/entry/delete/<%= entry.id %>">Delete</a></small>
        </div>
        
        <% end %>
        </div>
<% end %>
		
<% end %>
	
</div>

          <!-- Optional: clear the XS cols if their content doesn't match in height -->
        <div class="clearfix visible-xs-block"></div>
        <div class="col-md-4 hidden-sm hidden-xs">
